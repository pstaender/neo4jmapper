{
  "name": "neo4jmapper",
  "description": "Neo4jMapper is a client- and serverside object modeler for Neo4j graphdatabases",
  "version": "1.1.0",
  "author": "Philipp Staender <philipp.staender@gmail.com>",
  "homepage": "http://pstaender.github.io/neo4jmapper/",
  "main": "./src/index",
  "repository": {
    "type": "git",
    "url": "git://github.com:pstaender/neo4jmapper"
  },
  "keywords": [
    "neo4j",
    "graphdatabase",
    "orm",
    "graph",
    "database",
    "driver",
    "rest",
    "client"
  ],
  "scripts": {
    "test": "make test",
    "preinstall": "npm i -g coffee-script mocha-lcov-reporter istanbul",
    "blanket": {
      "pattern": "src"
    },
    "doc": "node_modules/docco/bin/docco src/*.js",
    "apidocs": "dox-foundation --source src --target apidocs",
    "prepare": "npm test && npm run doc && make test-coverage && npm run compress"
  },
  "devDependencies": {
    "mocha": "~2.2.5",
    "expect.js": "~0.3.1",
    "should": "~6.0.3",
    "futures": "~2.3.3",
    "join": "~2",
    "mocha-lcov-reporter": "0.0.1",
    "blanket": "~1.1.7",
    "jscoverage": "~0.5.9",
    "coffee-script": "~1.9.2",
    "optimist": "~0.6.1",
    "benchmark": "~1.0.0",
    "uglify-js": "~2.4.23",
    "istanbul": "^0.3.14",
    "docco": "~0.7.0"
  },
  "dependencies": {
    "JSONStream": "~0.9",
    "request": "~2.27.0",
    "underscore": "~1.8.3"
  },
  "licenses": {
    "type": "GPL"
  }
}
