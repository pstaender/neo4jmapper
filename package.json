{
  "name": "neo4jmapper",
  "description": "Neo4jMapper is a object modeler for Neo4j graphdatabases on NodeJS",
  "version": "2.0.0",
  "author": "Philipp Staender <philipp.staender@gmail.com>",
  "homepage": "http://pstaender.github.io/neo4jmapper/",
  "main": "./src/index",
  "repository": {
    "type": "git",
    "url": "git://github.com:pstaender/neo4jmapper"
  },
  "keywords": [
    "neo4j",
    "graphdatabase",
    "orm",
    "graph",
    "database",
    "driver",
    "rest",
    "client"
  ],
  "scripts": {
    "test": "mocha",
    "preinstall": "npm i -g coffee-script istanbul",
    "blanket": {
      "pattern": "src"
    },
    "doc": "node_modules/docco/bin/docco src/*.js",
    "apidocs": "node_modules/dox-foundation/bin/dox-foundation --source src --target apidocs",
    "prepare": "npm test; npm run doc; npm run apidocs; make test-coverage"
  },
  "devDependencies": {
    "mocha": "~2.2.5",
    "expect.js": "~0.3.1",
    "coffee-script": "~1.9.2",
    "istanbul": "~0.3.14",
    "docco": "~0.7.0",
    "dox-foundation": "^0.5.6"
  },
  "dependencies": {
    "JSONStream": "latest",
    "colors": "^1.1.0",
    "request": "latest",
    "underscore": "~1.8.3"
  },
  "licenses": {
    "type": "GPL"
  }
}
